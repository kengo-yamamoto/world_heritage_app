<h1>Complexes#index</h1>
<p>Find me in app/views/comprexes/index.html.erb</p>
<div class="container">
    <div class="row">
        <div class="col-12">
            <% @complexes.each do |item|%>
                <%= attachment_image_tag :item, fallback: "no_image.png", class: "rounded" %>
                <%= item.genre %><br />
                <%= item.country %><br />
                <%= item.name %><br />
                <%= link_to '詳しくはこちらへ', item_path %>
            <% end %>
        </div>
    </div>
</div>

